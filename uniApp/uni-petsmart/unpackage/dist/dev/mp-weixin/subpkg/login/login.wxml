<view><view class="sun-logo-box"><view class="sun-logo"><image class="sun-icon-img" src="/static/login/login_appIcon.png"></image></view></view><view class="sun-login-box"><view class="sun-label"><image style="width:28rpx;height:39rpx;" src="/static/login/mobile_icon.png"></image><text class="label-text">手机</text></view><view class="sun-input-box"><input type="text" placeholder="请输入手机号" placeholder-class="placeholder-class" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e"/><image class="close-icon" src="/static/login/close_icon.png" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="sun-login-box"><view class="sun-label"><image style="width:29rpx;height:37rpx;" src="/static/login/pwd_icon.png"></image><text class="label-text">密码</text></view><view class="sun-input-box"><input type="text" placeholder="请输入密码" placeholder-class="placeholder-class" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><image class="close-icon" src="/static/login/close_icon.png" data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="sun-tip"><text data-event-opts="{{[['tap',[['changeLoginType',['$event']]]]]}}" class="sun-tip-text" bindtap="__e">快捷登录</text><text data-event-opts="{{[['tap',[['forgetPassword',['$event']]]]]}}" class="sun-tip-text" bindtap="__e">忘记密码?</text></view><view class="login-btn-box"><view data-event-opts="{{[['tap',[['handleSubmit',['$event']]]]]}}" class="login-btn" bindtap="__e">登录</view><view data-event-opts="{{[['tap',[['goRegister',['$event']]]]]}}" class="register-btn" bindtap="__e">注册</view></view></view>